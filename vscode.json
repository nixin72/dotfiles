{
    //Window
    "window.title": "${dirty}${activeEditorLong}${separator}${rootName}${separator}${appName}",
    "window.restoreFullscreen": true,
    "window.newWindowDimensions": "maximized",
    //Workbench
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.restoreViewState": false,
    "workbench.editor.tabSizing": "shrink",
    "workbench.settings.editor": "json",
    "workbench.settings.useSplitJSON": true,
    "workbench.editor.openPositioning": "first",
    "workbench.startupEditor": "none",
    "workbench.colorTheme": "Solarized Dark",
    "workbench.statusBar.feedback.visible": false,
    "workbench.activityBar.visible": true,
    //Editor
    "editor.glyphMargin": false,
    "editor.minimap.enabled": false,
    "editor.wordWrap": "on",
    "editor.renderWhitespace": "boundary",
    "editor.tabCompletion": "on",
    "editor.rulers": [
        80,
        100
    ],
    "editor.wordSeparators": "_`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?",
    "editor.wordWrapColumn": 100,
    "editor.wrappingIndent": "indent",
    "editor.fontLigatures": true,
    "editor.formatOnPaste": true,
    "editor.renderControlCharacters": true,
    "editor.renderIndentGuides": true,
    "editor.renderLineHighlight": "all",
    "editor.dragAndDrop": true,
    "editor.formatOnSave": true,
    "editor.autoClosingQuotes": "always",
    "editor.autoClosingBrackets": "always",
    "editor.fontFamily": "Droid Sans Mono Dotted for Powerline, Consolas, 'Courier New', monospace",
    "editor.fontSize": 13,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.smoothScrolling": true,
    "editor.cursorWidth": 1,
    "editor.acceptSuggestionOnEnter": "smart",
    //Files
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 30000,
    "files.associations": {
        "*.es": "javascript",
        "*.inc": "php",
        "*.wsp": "clojure",
        "*.ic": "clojure"
    },
    "files.eol": "\n",
    "files.hotExit": "onExitAndWindowClose",
    "files.exclude": {
        "**/__pycache__": true,
        "**/.vscode": true,
        "**/node_modules": true
    },
    "files.autoGuessEncoding": true,
    "files.defaultLanguage": "js",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    //Terminal
    "terminal.external.windowsExec": "C:\\Program Files\\Git\\bin\\bash.exe",
    "terminal.integrated.shell.windows": "C:\\Windows\\System32\\bash.exe",
    "terminal.integrated.shellArgs.windows": [
        "-c",
        "fish"
    ],
    "terminal.integrated.rendererType": "dom",
    "terminal.integrated.fontFamily": "Droid Sans Mono Dotted for Powerline",
    "terminal.integrated.confirmOnExit": true,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.splitCwd": "initial",
    "terminal.integrated.windowsEnableConpty": false,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.scrollback": 500,
    //Python
    "python.autoComplete.addBrackets": true,
    "python.jediEnabled": false,
    "python.linting.enabled": false,
    "python.linting.pylintEnabled": false,
    //CSS/LESS
    "css.lint.float": "warning",
    "css.lint.idSelector": "warning",
    "css.lint.important": "warning",
    "css.lint.universalSelector": "warning",
    "less.lint.boxModel": "warning",
    "less.lint.duplicateProperties": "warning",
    "less.lint.float": "warning",
    "less.lint.universalSelector": "warning",
    //Misc
    "php.validate.executablePath": "C:/Program Files (x86)/PHP/v7.1/php.exe",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "search.exclude": {
        "**/node_modules": false
    },
    "debug.allowBreakpointsEverywhere": true,
    "debug.enableAllHovers": true,
    "debug.inlineValues": true,
    "breadcrumbs.enabled": true,
    "breadcrumbs.filePath": "last",
    "explorer.openEditors.visible": 0,
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "scm.diffDecorationsGutterWidth": 1,
    "scm.diffDecorations": "overview",
    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "eslint.alwaysShowStatus": true,
    "eslint.runtime": "C:/Program Files/nodejs/node.exe",
    "html.format.indentInnerHtml": true,
    "html.format.indentHandlebars": true,
    "html.format.wrapLineLength": 100,
    "java.completion.guessMethodArguments": true,
    "java.import.gradle.enabled": false,
    "java.import.maven.enabled": false,
    "markdown.extension.print.imgToBase64": true,
}
